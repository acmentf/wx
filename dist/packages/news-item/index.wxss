.news-item .item-wrap {
  font-size: 24rpx;
  padding: 30rpx;
  border-bottom: 1px solid #eee;
}
.news-item .item-wrap .remark {
  color: #d81e06;
  margin-right: 10rpx;
  padding: 2rpx 8rpx;
  border-left: 1rpx solid #d81e06;
  border-right: 1rpx solid #d81e06;
  font-size: 22rpx;
}
.news-item .item-wrap .item-info {
  display: flex;
}
.news-item .item-wrap .item-info view.item-time {
  text-align: left;
  flex: 1;
  line-height: 66rpx;
  color: #999;
}
.news-item .item-wrap .item-info view.item-handle {
  width: 150rpx;
  text-align: right;
}
.news-item .item-wrap .item-info view.item-handle > view {
  display: inline-block;
  color: #999;
}
.news-item .item-wrap.item-wrap-2 {
  display: flex;
}
.news-item .item-wrap.item-wrap-2 .item-left {
  width: 160rpx;
  height: 120rpx;
}
.news-item .item-wrap.item-wrap-2 .item-right {
  padding-left: 20rpx;
  line-height: 56rpx;
  flex: 1;
}
.news-item .item-wrap.item-wrap-2 .item-right .item-title {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 510rpx;
}
.news-item .item-wrap.item-wrap-4 {
  padding-bottom: 15rpx;
}
.news-item .item-wrap.item-wrap-4 .item-middle image {
  border-radius: 10rpx;
  margin-top: 12rpx;
  height: 4rem;
  vertical-align: center;
}
.news-item .item-wrap.item-wrap-4 .item-middle > .image-list {
  display: flex;
  padding-bottom: 20rpx;
}
.news-item .item-wrap.item-wrap-4 .item-middle > .image-list > view {
  flex: 1;
  padding: 10rpx;
}
.news-item .item-wrap.item-wrap-4 .item-bottom .item-info {
  line-height: 40rpx;
}
@font-face {
  font-family: 'ui-icon';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}
.iconfont {
  font-family: "ui-icon" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-cate:before {
  content: "\e615";
}
.icon-pl:before {
  content: "\e679";
}
.icon-dz:before {
  content: "\e60d";
}
.icon-r:before {
  content: "\e638";
}
