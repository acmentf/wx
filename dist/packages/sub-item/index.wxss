.sub-item {
  margin-bottom: 20rpx;
}
.sub-item .sub-title {
  display: flex;
  padding: 20rpx;
  border-bottom: 1px solid #eee;
}
.sub-item .sub-title .image {
  width: 40rpx;
  height: 40rpx;
}
.sub-item .sub-title .middle {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
}
.sub-item .sub-title .iconfont {
  width: 20rpx;
  height: 20rpx;
  display: inline-block;
}
@font-face {
  font-family: 'ui-icon';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}
.iconfont {
  font-family: "ui-icon" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-cate:before {
  content: "\e615";
}
.icon-pl:before {
  content: "\e679";
}
.icon-dz:before {
  content: "\e60d";
}
.icon-r:before {
  content: "\e638";
}
